	body
		nav.navbar.navbar-fixed-top.navbar-inverse
			.navbar-header
				button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
					span.sr-only Toggle navigation
					|             
					span.icon-bar
					|             
					span.icon-bar
					|             
					span.icon-bar
					|           
				a.navbar-brand(href='#')
					img.img-responsive(src='', alt='Gallery')
			#navbar.collapse.navbar-collapse
				ul.nav.navbar-nav
					li(class=(title=='Home' ? 'active':''))
						a(href='/') Home
							i.fa.fa-home(aria-hidden='true')
					li(class=(title=='About' ? 'active':''))
						a(href='/about') About Us
							i.fa.fa-picture-o(aria-hidden='true')
					if !user           
						li(class=(title=='Register' ? 'active':''))
							a(href='/users/register') Register
						
						|             
						li(class=(title=='Login' ? 'active':''))
							a(href='/users/login') Log In
				ul.nav.navbar-nav.navbar-right
					if user
						li
							a(href='/myprofile') Profile
						li
							a(href='/posts/add') Upload
						li
							a(href='/users/logout') Logout
		
		block content
			!= messages()
script(src='../../dist/js/bootstrap.js')
